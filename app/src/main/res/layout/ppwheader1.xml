<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:background="@color/f8"
                android:layout_height="100pt">


    <View android:layout_width="match_parent" android:layout_height="10pt"/>

    <com.yjhh.ppwbusiness.views.cui.IImageView android:layout_width="50pt" android:layout_height="50pt"
                                               android:id="@+id/iv_image"
                                               android:layout_centerHorizontal="true"
                                               android:layout_gravity="center"
                                               android:src="@drawable/icon_ppw"
                                               android:text="121212"
    />
    <View android:layout_width="match_parent" android:layout_height="10pt"/>




        <com.yjhh.ppwbusiness.views.cui.SportView android:layout_width="30pt"
                                                  android:id="@+id/sview"
                                                  android:layout_centerInParent="true"
                                                  android:layout_gravity="center"
                                                  android:layout_alignParentTop="true"
                                                  android:layout_height="30pt"
        />



</RelativeLayout>